@model Covid_19_WebSite.Models.Enumeration

@{
    ViewBag.Title = "Delete";
}

<h2 class="my-5">Supprimer une résulat </h2>


<div class="alert alert-dismissible alert-danger">
    <button type="button" class="close" data-dismiss="alert">&times;</button>
    <h4 class="alert-heading">Attention !!</h4>
    <p class="mb-0">Est ce que vous éte sure de supprimer cette résultat ?</p>
</div>

<div>

    <table class="table table-hover">
        <thead class="table-info">
        <th>
            @Html.DisplayNameFor(model => model.Cas_Mort)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Cas_Retablis)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Cas_Confirmer)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Ville.Nom)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Date_Ins)
        </th>
        </thead>
        <tbody>
            <tr class="table-secondary">
                <td>
                    @Html.DisplayFor(model => model.Cas_Mort)
                </td>
                <td>
                    @Html.DisplayFor(model => model.Cas_Retablis)
                </td>
                <td>
                    @Html.DisplayFor(model => model.Cas_Confirmer)
                </td>
                <td>
                    @Html.DisplayFor(model => model.Ville.Nom)
                </td>
                <td>
                    @Html.DisplayFor(model => model.Date_Ins)
                </td>
            </tr>
        </tbody>
    </table>

    @using (Html.BeginForm())
    {
        @Html.AntiForgeryToken()

        <div class="form-actions no-color">
            @Html.ActionLink("Supprimer", "Delete", new {id=Model.UID }, htmlAttributes: new { @class = "btn btn-danger btn-outline mr-4 bt_w30" }) 
            @Html.ActionLink("Liste", "Index", null, htmlAttributes: new { @class = "btn btn-outline-warning bt_w30" })
        </div>
    }
</div>
<br />
<br />
<br />
